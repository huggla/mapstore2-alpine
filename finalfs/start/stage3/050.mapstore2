# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
   if [ -s "$VAR_MAPSTORE_CONFIG_DIR/localConfig.json" ]
   then
      cp -a "$VAR_MAPSTORE_CONFIG_DIR/localConfig.json" "/usr/local/tomcat/webapps/mapstore2/"
   else
      cp -a "/usr/local/tomcat/webapps/mapstore2/localConfig.json" "$VAR_MAPSTORE_CONFIG_DIR/"
   fi
fi
